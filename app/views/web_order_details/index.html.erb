<p id="notice"><%= notice %></p>

<h1>Listing Web Order Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_order_details.each do |web_order_detail| %>
      <tr>
        <td><%= link_to 'Show', web_order_detail %></td>
        <td><%= link_to 'Edit', edit_web_order_detail_path(web_order_detail) %></td>
        <td><%= link_to 'Destroy', web_order_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web order detail', new_web_order_detail_path %>
